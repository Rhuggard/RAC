<% if @notice_queue %>
<%= form_for(@notice_queue) do |f| %>
  <% if @notice_queue.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@notice_queue.errors.count, "error") %> prohibited this notice_queue from being saved:</h2>

      <ul>
      <% @notice_queue.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :sender_id %><br />
