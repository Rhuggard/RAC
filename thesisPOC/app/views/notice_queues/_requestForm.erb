    <%= f.label :message %><br />
    <%= f.text_field :message,
         :placeholder => 'Write any additional information here...' %>
  </div>
  <div class="field">
    <%= f.label :request %><br />
    <% if request == true %>
      <%= f.text_field :request, :value => 'true' %>
    <% else %>
      <%= f.text_field :request, :value => 'false' %>
    <% end %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
