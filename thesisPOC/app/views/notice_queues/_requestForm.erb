  <div class="field">
    <%= f.label :action %><br />
    <%= f.text_field :action, :value => 'view' %>
  </div>
  <div class="field">
    <%= f.label :object_id %><br />
    <%= f.number_field :object_id, :value => pobject_id %>
  </div>
